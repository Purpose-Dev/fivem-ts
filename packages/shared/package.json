{
    "name": "@fivem-ts/shared",
    "version": "0.8.0",
    "author": "Purpose-Dev",
    "description": "FiveM Typescript wrapper shared part",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "lint": "eslint"
    },
    "keywords": [
        "fivem",
        "shared",
        "typescript",
        "javascript",
        "wrapper",
        "citizenfx",
        "natives",
        "library",
        "fivem-ts",
        "fivem-js"
    ],
    "devDependencies": {
        "@citizenfx/client": "2.0.10673-1",
        "@citizenfx/server": "2.0.10673-1"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./@types": {
            "types": "./dist/@types/index.d.ts",
            "default": "./dist/@types/index.js",
            "import": "./dist/@types/index.js",
            "require": "./dist/@types/index.js"
        },
        "./decorators": {
            "types": "./dist/decorators/index.d.ts",
            "default": "./dist/decorators/index.js",
            "import": "./dist/decorators/index.js",
            "require": "./dist/decorators/index.js"
        },
        "./enums": {
            "types": "./dist/enums/index.d.ts",
            "default": "./dist/enums/index.js",
            "import": "./dist/enums/index.js",
            "require": "./dist/enums/index.js"
        },
        "./hashes": {
            "types": "./dist/hashes/index.d.ts",
            "default": "./dist/hashes/index.js",
            "import": "./dist/hashes/index.js",
            "require": "./dist/hashes/index.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "default": "./dist/utils/index.js",
            "import": "./dist/utils/index.js",
            "require": "./dist/utils/index.js"
        }
    }
}
