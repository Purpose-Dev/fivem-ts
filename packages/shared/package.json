{
    "name": "@fivem-ts/shared",
    "version": "0.1.0",
    "author": "Purpose-Dev",
    "description": "FiveM Typescript wrapper shared part",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsc"
    },
    "keywords": [
        "fivem",
        "shared",
        "typescript",
        "javascript",
        "wrapper",
        "citizenfx",
        "natives",
        "library",
        "fivem-ts",
        "fivem-js"
    ],
    "devDependencies": {
        "@citizenfx/client": "2.0.10191-1",
        "@citizenfx/server": "2.0.10191-1",
        "@types/jest": "^29.5.13",
        "jest": "^29.7.0",
        "jest-junit": "^16.0.0",
        "ts-jest": "^29.2.5"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./enums": {
            "types": "./dist/enums/index.d.ts",
            "default": "./dist/enums/index.js",
            "import": "./dist/enums/index.js",
            "require": "./dist/enums/index.js"
        },
        "./hashes": {
            "types": "./dist/hashes/index.d.ts",
            "default": "./dist/hashes/index.js",
            "import": "./dist/hashes/index.js",
            "require": "./dist/hashes/index.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "default": "./dist/utils/index.js",
            "import": "./dist/utils/index.js",
            "require": "./dist/utils/index.js"
        }
    }
}
